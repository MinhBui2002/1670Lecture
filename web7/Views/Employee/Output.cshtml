@*
    receive data from EmployeeController and fetch into a table
*@
@model web7.Models.Employee

<div><
    <table class="table table-bordered">
        <tr>
            
            <th>Name</th>
            <th>Age</th>
            <th>Email</th>
            <th>Address</th>
            <th>Department</th>
            <th>Gender</th>
            <th>Salary</th>
        </tr>
        <tr>
            
            <td>@Model.Name</td>
            <td>@Model.Age</td>
            <td>@Model.Email</td>
            <td>@Model.Address</td>
            <td>@Model.Department</td>
            <td>@Model.Salary</td>
            <td>@Model.Gender</td>
        </tr>
    </table>
    
</div>


    